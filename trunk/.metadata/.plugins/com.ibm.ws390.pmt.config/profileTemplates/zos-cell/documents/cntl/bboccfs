           &DMFGHFS &WASHFSU &CNFGGRP &DMFSTYPE                                                                       
/*
)SEL &ASFSTYPE = HFS                                                                        
//ALLOCA   EXEC  PGM=IEFBR14                                                     
//CHFS    DD    UNIT=SYSALLDA,DISP=(NEW,CATLG),                                 
//        DSN=&CNFGHFS,                                                         
)SEL &CNFGVOL ^= *                                                              
//        VOL=SER=&CNFGVOL,                                                     
)ENDSEL                                                                         
//        DSNTYPE=HFS,SPACE=(CYL,(&CNFGPRI,&CNFGSEC,1)) 
)ENDSEL
)SEL &ASFSTYPE = ZFS
//DEFINEA  EXEC   PGM=IDCAMS                                                    
//SYSPRINT DD     SYSOUT=*                                                      
//SYSUDUMP DD     SYSOUT=*                                                      
//AMSDUMP  DD     SYSOUT=*
)SEL &CNFGVOL ^= *                                                      
//DASD0    DD     DISP=OLD,UNIT=3390,VOL=SER=&CNFGVOL
)ENDSEL                           
//SYSIN    DD     *                                                             
     DEFINE CLUSTER (NAME(&CNFGHFS) - 
)SEL &CNFGVOL ^= *                                                                                                
        VOLUMES(&CNFGVOL) -
)ENDSEL                                                     
        LINEAR CYL(&CNFGPRI &CNFGSEC) SHAREOPTIONS(3))                          
/*                                                                              
//FORMATA  EXEC   PGM=IKJEFT01,REGION=0M,COND=(0,LT)                            
//SYSTSPRT DD SYSOUT=*                                                          
//SYSTSIN  DD  *                                                                
 BPXBATCH SH +                                                                  
  zfsadm format -aggregate +                                                    
  '&CNFGHFS' +                                                                    
  -compat +                                                                     
  1>> /tmp/bboccfs_&TIMESTMP..out +                                            
  2>> /tmp/bboccfs_&TIMESTMP..err                                              
/*
)ENDSEL                        
//MNTAFS  EXEC  PGM=IKJEFT1A,COND=(0,LT)                                        
//* Build the mount point for the Config HFS                                    

)SEL &DMFSTYPE = HFS                                                                        
//ALLOCD   EXEC  PGM=IEFBR14                                                     
//CHFS    DD    UNIT=SYSALLDA,DISP=(NEW,CATLG),                                 
//        DSN=&DMFGHFS,                                                         
)SEL &DMFGVOL ^= *                                                              
//        VOL=SER=&DMFGVOL,                                                     
)ENDSEL                                                                         
//        DSNTYPE=HFS,SPACE=(CYL,(&DMFGPRI,&DMFGSEC,1))
)ENDSEL                         
)SEL &DMFSTYPE = ZFS
//DEFINED  EXEC   PGM=IDCAMS                                                    
//SYSPRINT DD     SYSOUT=*                                                      
//SYSUDUMP DD     SYSOUT=*                                                      
//AMSDUMP  DD     SYSOUT=*
)SEL &DMFGVOL ^= *                                                       
//DASD0    DD     DISP=OLD,UNIT=3390,VOL=SER=&DMFGVOL
)ENDSEL                       
//SYSIN    DD     *                                                             
     DEFINE CLUSTER (NAME(&DMFGHFS) -
)SEL &DMFGVOL ^= *                                          
        VOLUMES(&DMFGVOL) -                                                     
)ENDSEL
        LINEAR CYL(&DMFGPRI &DMFGSEC) SHAREOPTIONS(3))                          
/*                                                                              
//FORMATD  EXEC   PGM=IKJEFT01,REGION=0M,COND=(0,LT)                            
//SYSTSPRT DD SYSOUT=*                                                          
//SYSTSIN  DD  *                                                                
 BPXBATCH SH +                                                                  
  zfsadm format -aggregate +                                                    
  &DMFGHFS +                                                                    
  -compat +                                                                     
  1>> /tmp/bboccfs_&TIMESTMP..out +                                            
  2>> /tmp/bboccfs_&TIMESTMP..err                                              
/*
)ENDSEL
//MNTDFS  EXEC  PGM=IKJEFT1A,COND=(0,LT)                                        
//* Build the mount point for the Config HFS                                    
//SYSEXEC   DD DISP=SHR,DSN=&TRGHLQ..DATA                                       
//SYSTSPRT  DD SYSOUT=*                                                         
//SYSTSIN   DD *                                                                

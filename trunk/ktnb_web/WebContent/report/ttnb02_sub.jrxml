	<queryString>
		<![CDATA[select khn1.* from (select t.id_don_vi_th,
       t.ten_don_vi_th,
       decode(t.id_cuoc_ttkt_cha,'', t.ten_don_vi_bi,'    ' || t.ten_don_vi_bi) ten_don_vi_bi,
       decode(nd.ten_noi_dung,null,' ',nd.ten_noi_dung) ten_noi_dung,
       nvl(decode(t.hinh_thuc,0,'Thanh tra', 1, 'Kiểm tra'), decode(nd.hinh_thuc,0,'Thanh tra', 1, 'Kiểm tra ')) Ghi_chu,
       to_char(nvl(t.nien_do_tu_ngay, nd.nien_do_tu_ngay),'dd/mm/yyyy') || '-' || to_char(nvl(t.nien_do_den_ngay, nd.nien_do_den_ngay),'dd/mm/yyyy') nien_do
  from ttkt_kh_cuoc_ttkt t,
       (select id
          from ttkt_kh_cuoc_ttkt
         where loai_cuoc_ttkt_khn in ('KHN', 'TD', 'BS', 'TDTQD')
           and ma_trang_thai <> '1813'
           and ma like '%2011%'
           and id_don_vi_th like '00000'
           and loai = 0) khn,
       ttkt_kh_noi_dung_ttkt nd
 where (t.id = khn.id or t.id_cuoc_ttkt_cha = khn.id)
   and t.id  = nd.id_cuoc_ttkt(+)
   start with t.id_cuoc_ttkt_cha is null
connect by prior t.id = t.id_cuoc_ttkt_cha) khn1, ktnb_dm_cqt cqt
where khn1.id_don_vi_th = cqt.ma
order by cqt.ma]]>
	</queryString>
